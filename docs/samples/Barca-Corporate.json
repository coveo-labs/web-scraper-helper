[
  {
    "for": {
      "urls": [
        ".*"
      ]
    },
    "exclude": [
      {
        "type": "CSS",
        "path": "header, footer"
      }
    ],
    "metadata": {
      "objecttype": {
        "type": "CSS",
        "path": "[data-object-type]::attr(data-object-type)"
      },
      "description": {
        "type": "CSS",
        "path": ".cmp-peopledetail__bio,.cmp-detail__desc::text"
      },
      "articletags": {
        "type": "CSS",
        "path": ".cmp-articledetail__tags li a::text"
      },
      "expertise": {
        "type": "CSS",
        "path": ".cmp-peopledetail__expertise li a::text"
      },
      "division": {
        "type": "CSS",
        "path": "[data-division]::attr(data-division)"
      }
    }
  }
]
