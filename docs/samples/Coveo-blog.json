[
  {
    "for": {
      "urls": [
        ".*"
      ]
    },
    "exclude": [
      {
        "type": "CSS",
        "path": "head,header,aside,footer"
      },
      {
        "type": "CSS",
        "path": ".author>p"
      },
      {
        "type": "CSS",
        "path": ".topics"
      }
    ],
    "metadata": {
      "tags": {
        "type": "CSS",
        "path": ".topics li::text"
      },
      "title": {
        "type": "CSS",
        "path": ".title-container h1 a::text"
      }
    }
  }
]
